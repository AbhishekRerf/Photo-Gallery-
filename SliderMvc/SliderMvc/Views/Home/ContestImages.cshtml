@model IEnumerable<SliderMvc.Models.ContestImage>

@{
    ViewBag.Title = "ContestImages";
}

<h2 style="text-align:center">Contest Images</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.userid)
        </th>
        <th></th>
        <th></th>
        <th></th>
        <th></th>
        <th></th>
        <th></th>
        <th></th>
        <th></th>
        <th>
            @Html.DisplayNameFor(model => model.ImagePath)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.userid)
    </td>
    <th></th>
    <th></th>
    <th></th>
    <th></th>
    <th></th>
    <th></th>
    <th></th>
    <th></th>
    <td>
        <img src="@Url.Content(item.ImagePath)" height="300" width="320" style="padding:12px 12px;margin-left:40px;margin-top:15px;border:2px solid black;" />

    </td>

</tr>
}

</table>
